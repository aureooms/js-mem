{
  "name": "@aureooms/js-memory",
  "description": "Memory management code bricks for JavaScript",
  "version": "4.0.0",
  "author": "aureooms",
  "ava": {
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-memory/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-functools": "^2.0.3",
    "ava": "^0.19.0",
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "codeclimate-test-reporter": "^0.4.0",
    "coveralls": "^2.11.14",
    "esdoc": "^1.0.0-alpha.2",
    "nyc": "^10.0.0"
  },
  "homepage": "http://aureooms.github.io/js-memory",
  "keywords": [
    "allocation",
    "arrays",
    "javascript",
    "js",
    "management",
    "memory"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-memory.git",
